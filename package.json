{
  "name": "ilp-connector",
  "version": "18.0.1",
  "description": "ILP connector reference implementation",
  "repository": {
    "type": "git",
    "url": "git@github.com:interledgerjs/ilp-connector.git"
  },
  "keywords": [
    "interledger",
    "five-bells",
    "payments"
  ],
  "author": "Ripple <info@ripple.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/interledgerjs/ilp-connector/issues"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start-prof": "node --prof --logfile=${CONNECTOR_V8_LOGFILE:-v8.log} src/index.js",
    "start:watch": "nodemon src/index.js",
    "lint": "eslint src test scripts schemas",
    "test": "istanbul test -- _mocha",
    "report-coverage": "codecov",
    "integration": "integration-loader && integration all",
    "bump": "version-bump"
  },
  "engines": {
    "node": ">=6.6.0"
  },
  "dependencies": {
    "bignumber.js": "^3.0.1",
    "co": "^4.1.0",
    "co-defer": "^1.0.0",
    "co-request": "^1.0.0",
    "eventemitter2": "^2.2.1",
    "five-bells-shared": "interledgerjs/five-bells-shared#dj-ilqp",
    "ilp": "interledgerjs/ilp#dj-ilqp",
    "ilp-core": "^14.0.0",
    "ilp-packet": "~1.2.0",
    "ilp-plugin-bells": "interledgerjs/ilp-plugin-bells#dj-ilqp",
    "ilp-routing": "interledgerjs/ilp-routing#dj-ilqp",
    "lodash": "^4.6.1",
    "moment": "^2.10.2",
    "promise-retry": "^1.1.1",
    "riverpig": "^1.1.0",
    "sequelize": "^3.27.0",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "chai-as-promised": "^6.0.0",
    "co-mocha": "^1.1.0",
    "codecov": "^1.0.1",
    "eslint": "^3.11.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "five-bells-integration-test-loader": "^1.3.0",
    "istanbul": "^0.4.1",
    "mocha": "^3.2.0",
    "mock-require": "^2.0.0",
    "mock-socket": "^6.0.1",
    "nock": "^9.0.2",
    "nodemon": "^1.8.1",
    "sinon": "^2.1.0",
    "spec-xunit-file": "0.0.1-3",
    "sqlite3": "^3.1.4"
  },
  "config": {
    "five-bells-integration-test-loader": {
      "module": "five-bells-integration-test",
      "repo": "interledgerjs/five-bells-integration-test"
    }
  }
}
